{
  "name": "njk-html",
  "scopeName": "text.html.njk",
  "comment": "Nunjucks Template",
  "fileTypes": ["nj", "njk", "html","md"],
  "foldingStartMarker": "(<(?i:(head|table|tr|div|style|script|ul|ol|form|dl))\\b.*?>|{%\\s*(block|filter|for|if|macro|raw|switch))",
  "foldingStopMarker": "(</(?i:(head|table|tr|div|style|script|ul|ol|form|dl))\\b.*?>|{%\\s*(endblock|endfilter|endfor|endif|endmacro|endraw|endswitch)\\s*%})",
  "patterns": [
    {
      "include": "source.njk"
    },
    {
      "include": "text.html.basic"
    }
  ]
}
